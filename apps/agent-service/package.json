{
    "name": "agent-service",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
      "start": "node dist/index.js",
    "prisma:generate": "prisma generate --schema src/db/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema src/db/schema.prisma --name init",
    "prisma:deploy": "prisma migrate deploy --schema src/db/schema.prisma"
    },
    "dependencies": {
      "@foreign-trade-assistant/dto": "workspace:*",
      "@prisma/client": "^5.12.0",
      "axios": "^1.6.0",
      "dotenv": "^16.3.1",
      "fastify": "^4.25.0",
      "pino": "^9.0.0",
      "zod": "^3.22.0"
    },
    "devDependencies": {
      "prisma": "^5.12.0",
      "ts-node-dev": "^2.0.0",
      "tsup": "^7.2.0",
      "typescript": "^5.4.0",
      "pino-pretty": "^11.0.0"
    }
  }
  