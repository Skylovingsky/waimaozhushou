{
  "name": "agent-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate --schema src/db/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema src/db/schema.prisma --name init",
    "prisma:deploy": "prisma migrate deploy --schema src/db/schema.prisma"
  },
  "dependencies": {
    "@foreign-trade-assistant/dto": "workspace:*",
    "@prisma/client": "^5.12.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.29.1",
    "pino": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "24.5.1",
    "pino-pretty": "^11.3.0",
    "prisma": "^5.12.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0",
    "tsx": "^4.19.0"
  }
}